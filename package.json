{
  "name": "contler-hotel",
  "version": "0.0.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev": "concurrently \"npm:dev:build\" \"npm:dev:electron\"",
    "dev:build": "webpack --mode development --watch",
    "dev:electron": "wait-on dist-electron/main.js && electronmon .",
    "electron:build": "webpack --mode production && electron-builder build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "firebase": "^10.13.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2",
    "@aracna/fcm": "^1.0.28"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "electronmon": "^2.0.3",
    "esbuild-loader": "^4.2.2",
    "eslint": "^9.9.1",
    "eslint-webpack-plugin": "^4.2.0",
    "prettier": "^3.3.3",
    "ts-loader": "^9.5.1",
    "wait-on": "^8.0.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0",
    "@angular-devkit/build-angular": "^17.0.8",
    "@angular/cli": "^17.0.8",
    "@angular/compiler-cli": "^17.0.0",
    "@electron/remote": "^2.1.2",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^32.0.1",
    "electron-builder": "^24.13.3",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.2.2"
  }
